mixin pagination(paginationInfo, link = "")
    .pagination
        if paginationInfo.previous
          a.arrow(href=link + '/?page=' + paginationInfo.previous.page) 
            i.fas.fa-chevron-left
        if paginationInfo.first
          a.number(href=link + '/?page=1') 1
        if paginationInfo.less
          p ...
        each val in paginationInfo.tabOfNumbers
          if paginationInfo.page == val
            p.number#thisPage= val++
          else
            a.number(href=link + '/?page=' + val)= val++
        if paginationInfo.more
          p ...
        if paginationInfo.next
          a.arrow(href=link + '/?page=' + paginationInfo.next.page)
            i.fas.fa-chevron-right
